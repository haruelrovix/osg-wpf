<Window x:Class="OSG.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OSG.WPF"
        xmlns:prop="clr-namespace:OSG.WPF.Properties"
        mc:Ignorable="d"
        Title="{x:Static prop:Resources.WindowTitle}"
        SizeToContent="WidthAndHeight"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Closed="Window_Closed"
        Icon="osg.ico">
    <Grid>
        <WindowsFormsHost x:Name="windowsFormHost" Width="1024" Height="768" />
    </Grid>
</Window>
